{
  "name": "agent-skills-eval",
  "version": "1.0.0",
  "description": "Universal agent skills evaluation tool for Claude Code, OpenCode, and other platforms",
  "main": "lib/index.js",
  "bin": {
    "agent-skills-eval": "./bin/cli.js"
  },
  "scripts": {
    "discover": "node bin/cli.js discover",
    "eval": "node bin/cli.js eval",
    "report": "node bin/cli.js report",
