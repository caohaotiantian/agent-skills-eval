  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.0",
    "glob": "^10.3.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^3.3.0",
    "openai": "^4.104.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "jest": "^29.7.0"
  }
